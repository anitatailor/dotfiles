
Host bitbucket.org
  HostName bitbucket.org
  IdentityFile ~/.ssh/id_rsa_git

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_rsa_git

Host bastion
  HostName 13.126.0.23
  User sd5711
  ServerAliveInterval 60
  ServerAliveCountMax 120
  IdentityFile ~/.ssh/id_rsa

Host 10.0.* !bitbucket.org !github.com !bastion
  User sd5711
  ServerAliveInterval 60
  ServerAliveCountMax 120
  ProxyJump bastion
#  ProxyCommand ssh bastion nc %h %p  

